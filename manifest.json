{
    "manifest_version": 3,
    "name": "Kumandan Merenek",
    "version": "1.0",
    "description": "Savulun bre gafiller",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
      "default_popup": "hello.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.klanlar.org/**"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
    {
      "resources": ["injo.js"],
      "matches": ["<all_urls>"]
    }
  ]
  }
  