{
    "manifest_version": 2,
    "name": "Button Counter Extension",
    "version": "1.0",
    "description": "Counts the number of buttons on the current page.",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
      "default_popup": "hello.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://tr83.klanlar.org/**"],
        "js": ["content.js"]
      }
    ]
  }
  